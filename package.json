{
  "main": "AppWrapper.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "format": "npx prettier --write .",
    "lint": "eslint --ext .ts,.tsx,.js,.json ./",
    "lint-fix": "npm run lint -- --fix --quiet",
    "commit": "npm test && npm run format && npm run lint:fix",
    "env-fix": "env 2.5.5",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "apply-patches": "patch-package",
    "build-watch": "webpack -w",
    "build": "webpack",
    "emulate-host": "firebase emulators:start --only hosting",
    "web-dev": "npm run build && npm run emulate-host",
    "func-dev": "concurrently \"cd functions && npm run build:watch\" \"cd functions && firebase emulators:start --only functions\" \"npm run func-env\"",
    "func-env": "cross-env FUNC=true npm start"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.jpg$": "<rootDir>/tests/RelativeImageStub.js",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "expo": "^41.0.0",
    "expo-ads-admob": "~10.0.4",
    "expo-av": "~9.1.2",
    "expo-constants": "~10.1.3",
    "expo-file-system": "~11.0.2",
    "expo-firebase-recaptcha": "^1.3.0",
    "expo-haptics": "~10.0.0",
    "expo-image-manipulator": "~9.1.0",
    "expo-image-picker": "~10.1.4",
    "expo-linking": "~2.2.3",
    "expo-media-library": "~12.0.2",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-store-review": "~4.0.2",
    "expo-tracking-transparency": "^1.0.0",
    "expo-updates": "~0.5.4",
    "firebase": "8.2.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "phone": "^2.4.21",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-calendars": "^1.1268.0",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-get-random-values": "~1.7.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-modal": "^11.10.0",
    "react-native-paper": "^4.7.2",
    "react-native-reanimated": "~2.1.0",
    "react-native-root-siblings": "^4.1.1",
    "react-native-root-toast": "^3.2.1",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.2.3",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.16",
    "tslib": "^2.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^41.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "patch-package": "^6.4.7",
    "prettier": "2.2.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "private": true
}
