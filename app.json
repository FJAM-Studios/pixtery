{
  "expo": {
    "owner": "fjam-studios",
    "name": "Pixtery",
    "description": "A fun app that lets you turn your own pictures into jigsaw puzzles and share them with friends!",
    "slug": "pixtery",
    "privacy": "unlisted",
    "sdkVersion": "41.0.0",
    "version": "1.2.0",
    "platforms": ["android", "ios"],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#C490D1",
    "primaryColor": "#7D8CC4",
    "icon": "./assets/icon.png",
    "developmentClient": {
      "silentLaunch": true
    },
    "scheme": "pixtery",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C490D1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "bundleIdentifier": "com.fjamstudios.pixtery",
      "buildNumber": "1.2.0",
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "***REMOVED***"
      },
      "associatedDomains": ["applinks:pixtery.io"],
      "infoPlist": {
        "NSCameraUsageDescription": "This app accesses your device camera so that you can take pictures with your camera to send as puzzles to others.",
        "NSPhotoLibraryUsageDescription": "This app accesses your device photo library so that you can select existing photos on your device to send as puzzles to others.",
        "NSUserTrackingUsageDescription": "Apple requires us to ask this because we show ads. Pixtery does NOT track your activity. You can safely click no and will still receive the full experience."
      }
    },
    "android": {
      "package": "com.fjamstudios.pixtery",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#C490D1"
      },
      "config": {
        "googleMobileAdsAppId": "***REMOVED***"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "pixtery.io",
            "pathPrefix": "/p"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Pixtery",
      "shortName": "Pixtery"
    }
  }
}
